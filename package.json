{
  "name": "@duetds/date-picker",
  "version": "1.0.0",
  "description": "Duet Date Picker is an open source version of Duet Design System’s Date Picker. It’s a Web Component that lets user pick a single date or a date range using a special calendar like date picker interface.",
  "author": "LocalTapiola Services Ltd <duetdesignsystem@lahitapiola.fi>",
  "license": "MIT",
  "module": "lib/index.mjs",
  "es2015": "lib/esm/index.mjs",
  "es2017": "lib/esm/index.mjs",
  "jsnext:main": "lib/esm/index.mjs",
  "main": "lib/index.js",
  "unpkg": "lib/duet/duet.js",
  "types": "lib/types/components.d.ts",
  "collection": "lib/collection/collection-manifest.json",
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "lib": "lib",
    "hydrate": "hydrate"
  },
  "files": [
    "lib",
    "hydrate"
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "stencil build --dev --watch --serve",
    "build": "stencil build --es5",
    "version": "npm run build",
    "precommit": "stencil test --spec --silent",
    "prepublish": "npm run build",
    "test": "stencil test --spec --e2e --silent",
    "test:dev": "stencil test --spec --e2e --watchAll",
    "test:unit": "stencil test --spec --silent",
    "test:e2e": "stencil test --e2e --silent"
  },
  "bugs": {
    "email": "duetdesignsystem@lahitapiola.fi"
  },
  "devDependencies": {
    "@duetds/stencil-angular-output-target": "0.0.2",
    "@rollup/plugin-replace": "2.3.1",
    "@stencil/core": "1.17.3",
    "@stencil/react-output-target": "0.0.9",
    "@stencil/sass": "1.3.2",
    "@stencil/utils": "latest",
    "@types/jest": "26.0.4",
    "@types/jest-image-snapshot": "3.1.0",
    "@types/puppeteer": "3.0.1",
    "jest": "26.1.0",
    "jest-cli": "26.1.0",
    "jest-image-snapshot": "4.1.0",
    "puppeteer": "5.2.1"
  }
}
